{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng"
        },
        {
            "name": "gongxsd-cli read books",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/gongxsd-cli",
            "args": [
                "read",
                "../../../test/books/books.xsd",
            ]
        },
        {
            "name": "gongxsd-cli read musicxml",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/gongxsd-cli",
            "args": [
                "read",
                "../../../test/musicxml/musicxml.xsd"
            ]
        },
        {
            "name": "go read musicxml",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/gongxsd-cli",
            "args": [
                "read",
                "../../../test/musicxml/musicxml.xsd"
            ]
        },
    ]
}